<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dra SYSTEM "dra.dtd">
<dra>
  <states>
    <state>
      <id>q1</id>
      <available-registers/>
    </state>
    <state>
      <id>q2</id>
      <available-registers>
        <register>3</register>
      </available-registers>
    </state>
    <state>
      <id>q3</id>
      <available-registers>
        <register>2</register>
        <register>3</register>
      </available-registers>
    </state>
    <state>
      <id>q4</id>
      <available-registers>
        <register>1</register>
        <register>2</register>
        <register>3</register>
      </available-registers>
    </state>
  </states>
  <initial-state>q1</initial-state>
  <transitions>
    <transition>
      <from>q1</from>
      <input>push</input>
      <op>LFresh</op>
      <register>3</register>
      <to>q2</to>
    </transition>
    <transition>
      <from>q2</from>
      <input>push</input>
      <op>LFresh</op>
      <register>2</register>
      <to>q3</to>
    </transition>
    <transition>
      <from>q3</from>
      <input>push</input>
      <op>LFresh</op>
      <register>1</register>
      <to>q4</to>
    </transition>
    <transition>
      <from>q2</from>
      <input>pop</input>
      <op>Read</op>
      <register>3</register>
      <to>q1</to>
    </transition>
    <transition>
      <from>q3</from>
      <input>pop</input>
      <op>Read</op>
      <register>2</register>
      <to>q2</to>
    </transition>
    <transition>
      <from>q4</from>
      <input>pop</input>
      <op>Read</op>
      <register>1</register>
      <to>q3</to>
    </transition>
  </transitions>
</dra>

